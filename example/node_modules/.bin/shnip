#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ndejesus/code/shnip/package/dist/bin/node_modules:/Users/ndejesus/code/shnip/package/dist/node_modules:/Users/ndejesus/code/shnip/package/node_modules:/Users/ndejesus/code/shnip/node_modules:/Users/ndejesus/code/node_modules:/Users/ndejesus/node_modules:/Users/node_modules:/node_modules:/Users/ndejesus/code/shnip/example/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ndejesus/code/shnip/package/dist/bin/node_modules:/Users/ndejesus/code/shnip/package/dist/node_modules:/Users/ndejesus/code/shnip/package/node_modules:/Users/ndejesus/code/shnip/node_modules:/Users/ndejesus/code/node_modules:/Users/ndejesus/node_modules:/Users/node_modules:/node_modules:/Users/ndejesus/code/shnip/example/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shnip/dist/bin/cli.js" "$@"
else
  exec node  "$basedir/../shnip/dist/bin/cli.js" "$@"
fi
